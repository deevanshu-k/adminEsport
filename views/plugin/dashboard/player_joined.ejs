<div class="player-joined">
    <div class="head">Player Joined</div>
    <div class="table">
        <table>
            <thead>
                <tr>
                    <th>Index</th>
                    <th>Type</th>
                    <th>Enrolled</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < data.length; i++) { %>
                    <% if (data.length === 0) { %>
                        <% exit; %>
                     <% } %>
                    <% if (data[i].status <= 0) { %>
                       <% continue; %>
                    <% } %>
                    <tr>
                        <td><%= data[i].indexs %> </td>
                        <td><%= data[i].type %></td>
                        <td><%= data[i].player_joined %> </td>
                    </tr>
                <%}%>
            </tbody>
        </table>
    </div>
</div>